{
  "name": "surfkit",
  "version": "0.0.4-alpha",
  "description": "Open source UI framework for use in SurfScape's websites and services",
  "main": "/css/css.css",
  "scripts": {
    "build-css": "cleancss -o \"dist/css.min.css\" \"css/css.css\"",
    "build-js": "uglifyjs -o \"dist/js.min.js\" \"js/*.js\"",
    "build-dist": "npm run build-css && npm run build-js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/surfscape/surfkit.git"
  },
  "keywords": [
    "ui",
    "framework",
    "css"
  ],
  "author": "SurfScape Team",
  "license": "GPL-2.0",
  "bugs": {
    "url": "https://github.com/surfscape/surfkit/issues"
  },
  "homepage": "https://github.com/surfscape/surfkit#readme",
  "devDependencies": {
    "clean-css-cli": "^5.6.2",
    "uglify-js": "^3.17.4"
  }
}
